set term svg font ",16" background rgb "white"
set tics out
set xlabel "x"
set ylabel "y"
set key top left
set title "Linear interpolation of data from f(x) = x^2 + x + 4"
set out "Linterp.svg"
plot "tabval.txt" with points pt 7 ps 0.6 lt rgb "blue" title "Table values"\
,"exactfuncs.txt" using 1:2 with lines lt rgb "red" title "Exact f(x)"\
,"out.txt" using 1:2 with lines lt rgb "black" title "Linear interpolation"
