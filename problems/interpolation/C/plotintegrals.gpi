set term svg font ",16" background rgb "white"
set tics out
set xlabel "x"
set ylabel "y"
set key top left
set title "Integral of f(x) = x^2 + 2*x + 1 + 25*sin(x) from x[0] to x"
set out "Integralvalues.svg"
plot "exactfuncs.txt" using 1:3 with lines lw 1.4 lt rgb "red" title "Exact integral of f(x)"\
,"out.txt" using 1:3 with lines lt rgb "black" title "Integral of qspline"
